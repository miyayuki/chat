<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<ul class="users">
	<% @users.each do |user| %>
		<li>
			User ID: <%= user.id %>
			<ul>
				<li>
					<%= user.email %>
				</li>
				<li>
					<%= l user.created_at.in_time_zone('Tokyo'), format: :datetime %>
				</li>
			</ul>
		</li>
	<% end %>
</ul>
